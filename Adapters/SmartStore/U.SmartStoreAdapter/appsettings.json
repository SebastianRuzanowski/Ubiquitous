{
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbOptions":
  {
    "Connection": "Server=localhost;Port=5432;Database=SmartDb;User Id=postgres;\nPassword=postgres;",
    "Type": "Npgsql",
    "AutoMigration": false
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "smartstore-adapter",
    "address": "docker.for.win.localhost",
    "port": "5004",
    "pingEnabled": true,
    "pingEndpoint": "api/smartstore/health",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "smartstore-adapter",
    "requestRetries": 3
  }
}
