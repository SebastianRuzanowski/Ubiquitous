{
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": "localhost:9600",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "U.ProductService": "Information",
        "System": "Warning"
      }
    } 
  },
  "AllowedHosts": "*",
  "DbOptions": {
    "Connection": "Server=localhost;Port=5432;Database=U.ProductService6;User Id=postgres;\nPassword=postgres;",
    "Type": "Npgsql",
    "AutoMigration": true,
    "Seed": true
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "u.product-service",
    "address": "docker.for.win.localhost",
    "port": "5003",
    "pingEnabled": true,
    "pingEndpoint": "api/product/health",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "u.product-service",
    "requestRetries": 3
  },
  "rabbit": {
    "subscriptionClientName": "Product",
    "eventBusConnection": "localhost",
    "eventBusRetryCount": 5,
    "eventBusUserName": "guest",
    "eventBusPassword": "guest"
  },
  "restEase": {
    "loadBalancer": "fabio",
    "services": []
  }
}
