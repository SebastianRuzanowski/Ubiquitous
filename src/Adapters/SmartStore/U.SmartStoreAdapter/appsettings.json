{
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "OpenTracing.Contrib": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "dbOptions":
  {
    "Connection": "Server=localhost;Port=5432;Database=SmartDb;User Id=postgres;\nPassword=postgres;",
    "Type": "Npgsql",
    "AutoMigration": false
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "u.smartstore-adapter",
    "address": "docker.for.win.localhost",
    "port": "5101",
    "pingEnabled": true,
    "pingEndpoint": "api/smartstore/health",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "u.smartstore-adapter",
    "requestRetries": 3
  },
  "swagger":
  {
    "title": "SmartStore Adapter HTTP API",
    "version": "v1",
    "description": "SmartStore Adapter HTTP API",
    "contact": "Contact with sebastian.ruzanowski@gmail.com",
    "license": "MIT"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": true,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "ubiquitous",
    "env": "local",
    "interval": 5
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "u.smartstore-adapter",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0
  }
}
