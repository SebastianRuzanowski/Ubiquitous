{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "OpenTracing.Contrib": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "dbOptions": {
    "Connection": "Server=docker.for.win.localhost;Port=5433;Database=smartstore-adapter;User Id=postgres;\nPassword=postgres;",
    "Type": "Npgsql",
    "autoMigration": false
  },
  "consul": {
    "enabled": true,
    "url": "http://consul:8500",
    "service": "u.smartstore-adapter",
    "address": "u.smartstore-adapter",
    "port": "5100",
    "pingEnabled": true,
    "pingEndpoint": "api/smartstore/health",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://fabio:9999",
    "service": "u.smartstore-adapter",
    "requestRetries": 3
  },
  "influx": {
    "enabled": true,
    "Uri": "http://influxdb:8086",
    "database": "ubiquitous",
    "env": "local",
    "interval": 5
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "u.smartstore-adapter",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0
  }
}
