{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "u.notification-service",
    "address": "docker.for.win.localhost",
    "port": "5500",
    "pingEnabled": true,
    "pingEndpoint": "api/notification/health",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "u.notification-service",
    "requestRetries": 3
  },
  "rabbit": {
    "SubscriptionClientName": "Notification",
    "EventBusConnection": "localhost",
    "EventBusRetryCount": 5,
    "EventBusUserName": "guest",
    "EventBusPassword": "guest"
  },
  "DbOptions":
  {
    "connection": "Server=localhost;Port=5432;Database=notification-service4;User Id=postgres;\nPassword=postgres;",
    "type": "Npgsql",
    "autoMigration": true
  },
  "redis":
  {
    "host": "127.0.0.1",
    "port": "6379"
  }
}