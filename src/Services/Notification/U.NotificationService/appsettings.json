{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "U.NotificationService.Controllers.HealthController": "Warning",
        "OpenTracing.Contrib": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "u.notification-service",
    "address": "docker.for.win.localhost",
    "port": "5500",
    "pingEnabled": true,
    "pingEndpoint": "api/notification/health",
    "pingInterval": 30,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "u.notification-service",
    "requestRetries": 3
  },
  "rabbit": {
    "SubscriptionClientName": "Notification",
    "EventBusConnection": "localhost",
    "EventBusRetryCount": 5,
    "EventBusUserName": "guest",
    "EventBusPassword": "guest"
  },
  "dbOptions":
  {
    "connection": "Server=localhost;Port=5432;Database=notification-service3;User Id=postgres;\nPassword=postgres;",
    "type": "Npgsql",
    "autoMigration": true
  },
  "redis":
  {
    "host": "127.0.0.1",
    "port": "6379",
    "serviceName": "notification-service:",
    "resolveDns": true,
    "abortOnConnectFail": false,
    "connectRetry": 10,
    "allowAdmin": true
  },
  "signalr":
  {
    "redisConnectionString": "127.0.0.1:6379"
  },
  "swagger":
  {
    "title": "Notification Service HTTP API",
    "version": "v1",
    "description": "Notification Service HTTP API",
    "contact": "Contact with sebastian.ruzanowski@gmail.com",
    "license": "MIT"
  },
  "jwt": {
    "secretKey": "super-secret-ubiquitous-key-!",
    "expiryMinutes": 30,
    "issuer": "identity-service",
    "validateLifetime": true
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": true,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "ubiquitous",
    "env": "local",
    "interval": 5
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "u.notification-service",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0
  }
}