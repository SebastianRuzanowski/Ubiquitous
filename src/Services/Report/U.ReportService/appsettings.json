{
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": "localhost:9600",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "U.ReportService": "Information",
        "System": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "u.Report-service",
    "address": "docker.for.win.localhost",
    "port": "5003",
    "pingEnabled": true,
    "pingEndpoint": "api/Report/health",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "u.report-service",
    "requestRetries": 3
  },
  "rabbit": {
    "subscriptionClientName": "Report",
    "eventBusConnection": "localhost",
    "eventBusRetryCount": 5,
    "eventBusUserName": "guest",
    "eventBusPassword": "guest"
  },
  "restEase": {
    "loadBalancer": "fabio",
    "services": []
  },
  "caracan":{
    "connection":"ws://127.0.0.1:3000"
  }
}
